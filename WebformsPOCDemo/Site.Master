<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Site.master.cs" Inherits="WebformsPOCDemo.Site" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title>Demo Document Manager</title>
    <link href="/CSS/Main.css" rel="stylesheet" />
